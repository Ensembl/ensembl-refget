{
    "listeners": {
        "*:8000": {
            "pass": "applications/fastapi"
        }
    },

    "applications": {
        "fastapi": {
            "type": "python 3.11",
            "path": [
                "/www/unit/",
                "/usr/lib/python3/dist-packages"
            ],
            "module": "main",
            "callable": "app",
            "working_directory": "/www/unit/"
        }
    }
}
